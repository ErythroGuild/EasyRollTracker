<Ui xmlns="http://www.blizzard.com/wow/ui/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

	<Frame name="eRollTracker_Template_Frame_Entry" virtual="true" enableMouse="true">
		<Anchors>
			<Anchor point="TOP"/>
			<Anchor point="LEFT" x="4"/>
			<Anchor point="RIGHT" x="-4"/>
			<Anchor point="BOTTOM" relativePoint="TOP" y="-24"/>
		</Anchors>

		<Layers>
			<Layer level="HIGHLIGHT">
				<Texture setAllPoints="true">
					<Color r="0.85" g="0.85" b="0.85" a="0.20"/>
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<FontString name="$parent_Role" inherits="GameFontNormal" text="#">
					<Size><AbsDimension x="24" y="24"/></Size>
					<Anchors><Anchor point="LEFT" x="4"/></Anchors>
				</FontString>
				<FontString name="$parent_Spec" inherits="GameFontNormal" text="HLY" justifyH="LEFT">
					<Size><AbsDimension x="36" y="24"/></Size>
					<Anchors><Anchor point="LEFT" relativeTo="$parent_Role" relativePoint="RIGHT"/></Anchors>
				</FontString>
				<FontString name="$parent_Max" inherits="GameFontNormal" text="100">
					<Size><AbsDimension x="50" y="24"/></Size>
					<Anchors><Anchor point="RIGHT" x="-14"/></Anchors>
				</FontString>
				<FontString name="$parent_Roll" inherits="GameFontNormal" text="42">
					<Size><AbsDimension x="40" y="24"/></Size>
					<Anchors><Anchor point="RIGHT" relativeTo="$parent_Max" relativePoint="LEFT"/></Anchors>
				</FontString>
				<FontString name="$parent_Name" inherits="GameFontNormal" text="Adeluna">
					<Size><AbsDimension x="0" y="24"/></Size>
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parent_Spec" relativePoint="RIGHT"/>
						<Anchor point="RIGHT" relativeTo="$parent_Roll" relativePoint="LEFT"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
	</Frame>
</Ui>